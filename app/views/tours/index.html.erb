<h1> All Tours </h1>

<% @tours.each do |tour| %>
<div class="card-product" id="index-list">
  <img src='<%= tour.photo_url %>' >
  <div class="card-product-infos">
    <h2><%= tour.name %></h2>
    <p><%= link_to 'See Details', tour_path(tour) %></p>
  </div>
</div>
<% end %>
<div class="btn-container">
<%= link_to "Host a new tour", new_tour_path, class: 'btn btn-primary' %>
</div>
